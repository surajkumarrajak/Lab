pipeline{
    agent any
    {
        tools{
            maven
            jdk
        }
        stages{

            stage ('Initialize') {
            steps {
                sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                ''' 
            }
        }
            stage ('Build') {
                steps {
                    sh """
                    echo "Building code"
                    """
                }
            }
            stage ('Test'){
            steps{
                sh """
                echo "testing code"
                """

            } 
            }
        }
    }
}
